{
    "name": "paypaplane/symfony-one",
    "description": "Fork of symfony 1.4 with dic, form enhancements, latest swiftmailer and better performance",
    "type": "library",
    "license": "MIT",
    "require": {
        "php" : ">=5.3.0",
        "swiftmailer/swiftmailer": "~5.2 || ^6.0"
    },
    "require-dev": {
        "psr/log": "*"
    },
    "autoload": {
        "files": ["autoload.php"]
    },
    "suggest": {
        "paypaplane/doctrine1": "Doctrine plugin",
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    },
    "replace": {
        "lexpress/symfony1": "^1.5"
    },
    "bin": ["data/bin/symfony"]
}
